{
  "name": "blog.sugarshin.net",
  "version": "0.0.0",
  "description": "blog",
  "private": true,
  "scripts": {
    "na": "node scripts/new-article",
    "mr": "node scripts/create-monthly-report",
    "dev": "npm run start:dev",
    "start:dev": "npm-run-all clean:dev mkdir:dev articles-index:dev --parallel cp:dev watch:copy-articles dev-server",
    "clean": "run() { if [ -d $1 ]; then rm -rf $1; fi; }; run",
    "clean:dev": "npm run clean -- build-dev",
    "clean:e2e-test": "npm run clean -- build-e2e-test",
    "clean:wwp-cache": "if [ -d .wwp-cache ]; then rm -rf .wwp-cache; fi",
    "mkdirp": "run() { mkdirp $1; }; run",
    "mkdir:dev": "npm run mkdirp -- build-dev",
    "mkdir:e2e-test": "npm run mkdirp -- build-e2e-test",
    "cp:build": "npm-run-all --parallel cp:build:image",
    "cp:build:image": "cpx 'articles/assets/**/*.{jpg,jpeg,png,gif,svg}' build/assets",
    "cp:dev": "npm-run-all --parallel cp:dev:article-images",
    "cp:dev:article-images": "cpx 'articles/assets/**/*.{jpg,jpeg,png,gif,svg}' build-dev/assets -w",
    "cp:e2e-test": "npm-run-all --parallel cp:e2e-test:article-images",
    "cp:e2e-test:article-images": "cpx 'articles/assets/**/*.{jpg,jpeg,png,gif,svg}' build-e2e-test/assets",
    "cp:review-app": "npm-run-all --parallel cp:review-app:article-images",
    "cp:review-app:article-images": "cpx 'articles/assets/**/*.{jpg,jpeg,png,gif,svg}' build-review-app/assets",
    "watch:copy-articles": "watch 'npm run copy-articles' articles",
    "copy-articles": "node scripts/copy-articles",
    "articles-index:dev": "node scripts/articles-index --out=build-dev",
    "articles-index:e2e-test": "node scripts/articles-index --out=build-e2e-test",
    "dev-server": "DEV_SERVER=1 webpack serve --config config/webpack.config.js --progress",
    "lint:js": "eslint --max-warnings=0 src test scripts config markdown",
    "lint:styl": "stylint src",
    "lint:md": "textlint articles/*.md",
    "lint": "run-p lint:*",
    "test": "NODE_PATH=src:node_modules jest --verbose",
    "test:coverage": "npm test -- --coverage",
    "test:ci": "npm run test:coverage -- --maxWorkers=2",
    "e2e-test": "jest -c e2e-test/jest.config.js",
    "e2e-test:ci": "npm run e2e-test -- --maxWorkers=2",
    "templates:build": "node scripts/templates",
    "webpack": "webpack --config config/webpack.config.js",
    "build": "/bin/bash scripts/build.bash",
    "build:e2e-test": "/bin/bash scripts/build-e2e-test.bash",
    "build:review-app": "/bin/bash scripts/build-review-app.bash",
    "postbuild:review-app": "bin/react-snap --source=build-review-app --cns",
    "deploy": "/bin/bash scripts/deploy.bash"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sugarshin/blog.sugarshin.net.git"
  },
  "author": {
    "name": "Shingo Sato",
    "email": "shinsugar@gmail.com",
    "url": "https://sugarshin.net/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sugarshin/blog.sugarshin.net/issues"
  },
  "homepage": "https://github.com/sugarshin/blog.sugarshin.net#readme",
  "engines": {
    "node": "12.x"
  },
  "jest": {
    "testRegex": "/test/.+\\.spec\\.js$",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/__mocks__/fileMock.js",
      "\\.(css|styl)$": "<rootDir>/test/__mocks__/styleMock.js"
    },
    "coverageDirectory": "./coverage/",
    "collectCoverageFrom": [
      "**/*.js",
      "!**/node_modules/**",
      "!**/vendor/**",
      "!**/config/**",
      "!**/build/**",
      "!**/build-dev/**",
      "!**/coverage/**",
      "!postcss.config.js"
    ],
    "setupFiles": [
      "raf/polyfill",
      "<rootDir>/config/configure-enzyme.js"
    ]
  },
  "browserslist": [
    "last 2 Android versions",
    "last 2 Chrome versions",
    "last 2 ChromeAndroid versions",
    "last 2 Edge versions",
    "last 2 Firefox versions",
    "last 2 iOS versions",
    "last 2 Safari versions"
  ],
