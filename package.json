{
  "name": "log.sugarshin.net",
  "version": "1.0.0",
  "description": "blog",
  "private": true,
  "scripts": {
    "na": "node scripts/new-article",
    "start": "opn http://localhost:8003/ && npm-run-all clean:dev mkdir:dev articles-index:dev --parallel pug:dev cp:dev watch:dev:articles dev-server",
    "clean:dev": "if [ -d build-dev ]; then rm -rf build-dev; fi",
    "mkdir:dev": "mkdirp build-dev",
    "cp:dev": "npm-run-all --parallel cp:dev:article-images",
    "cp:dev:article-images": "cpx 'articles/assets/**/*.{jpg,jpeg,png,gif,svg}' build-dev/assets -w",
    "watch:dev:articles": "watch 'npm run dev-articles' articles",
    "dev-articles": "node scripts/dev-articles",
    "articles-index:dev": "node scripts/articles-index --out=build-dev",
    "pug:dev": "pug -o build-dev -P -O '{\"title\": \"log | development\", \"lang\": \"ja\"}' -w src/template/index.pug",
    "dev-server": "DEV_SERVER=1 webpack-dev-server --config config/webpack.config.js --progress",
    "lint:js": "eslint src",
    "lint:styl": "stylint src",
    "lint:md": "textlint articles/*.md",
    "lint": "npm-run-all lint:*",
    "test-src:components": "jest",
    "test-src": "npm-run-all test-src:*",
    "test": "npm-run-all lint test-src",
    "clean:build": "if [ -d build ]; then rm -rf build; fi",
    "templates:build": "node scripts/templates",
    "webpack": "webpack --config config/webpack.config.js",
    "build": "sh scripts/build.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sugarshin/log.sugarshin.net.git"
  },
  "engines": {
    "node": ">=6"
  },
  "author": "sugarshin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sugarshin/log.sugarshin.net/issues"
  },
  "homepage": "https://github.com/sugarshin/log.sugarshin.net#readme",
  "jest": {
    "testRegex": "/test/.+\\.spec\\.js$",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/__mocks__/fileMock.js",
      "\\.(css|styl)$": "<rootDir>/test/__mocks__/styleMock.js"
    }
  },
